package dev.thinke.domain.sort.alg.quick;


@org.checkerframework.framework.qual.AnnotatedFor(
    "org.checkerframework.checker.nullness.KeyForSubchecker")
public record Partition(Integer startIndex, Integer endIndex) {

  @org.checkerframework.dataflow.qual.Pure
  public Integer size(Partition this) {
    return (this.endIndex - this.startIndex) + 1;
  }
}
